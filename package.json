{
  "name": "orlovsky.rocks",
  "version": "0.3.3",
  "description": "Zach Orlovsky's homepage",
  "scripts": {
    "start:dev": "babel-node server/dev.js",
    "start:prod": "npm run build && node dist/server.bundle.js",
    "build:server": "webpack --config webpack/webpack.server.config.js",
    "build:client": "webpack -p --config webpack/webpack.prod.config.js",
    "build": "npm run build:client && npm run build:server",
    "test": "BABEL_DISABLE_CACHE=1 NODE_ENV=test ava 'src/**/*.test.js' --verbose --require ./test/_helper.js"
  },
  "ava": {
    "babel": {
      "presets": [
        "es2015",
        "stage-0",
        "react"
      ]
    }
  },
  "author": "Zach Orlovsky <sadorlovsky@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.14.0",
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-webpack-loaders": "^0.4.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "css-loader": "^0.23.1",
    "enzyme": "^2.2.0",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "if-env": "^1.0.0",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "lost": "^6.7.2",
    "postcss-cssnext": "^2.5.2",
    "postcss-font-magician": "^1.4.0",
    "postcss-import": "^8.1.0",
    "postcss-loader": "^0.8.2",
    "react-addons-test-utils": "^15.0.1",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.13.1",
    "sugarss": "^0.1.0",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "dependencies": {
    "express": "^4.13.4",
    "moment": "^2.12.0",
    "react": "^15.0.2",
    "react-css-modules": "^3.7.5",
    "react-document-title": "^2.0.1",
    "react-dom": "^15.0.2",
    "react-icons": "github:gorangajic/react-icons",
    "react-markdown": "^2.1.1",
    "react-redux": "^4.4.2",
    "react-router": "^2.0.1",
    "react-router-redux": "^4.0.2",
    "redux": "^3.4.0",
    "redux-actions": "^0.9.1",
    "redux-thunk": "^2.0.1"
  }
}
