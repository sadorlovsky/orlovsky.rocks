{
  "name": "orlovsky.rocks",
  "version": "0.4.1",
  "description": "Zach Orlovsky's homepage",
  "scripts": {
    "start:dev": "babel-node src/server/dev.js",
    "start:prod": "npm run build && node dist/server.bundle.js",
    "build:server": "webpack --config webpack/webpack.config.server.js",
    "build:client": "webpack -p --config webpack/webpack.config.prod.js",
    "build": "npm run build:client && npm run build:server",
    "lint": "eslint src test",
    "test": "CONFIG=$(pwd)/webpack/webpack.config.test.js BABEL_DISABLE_CACHE=1 NODE_ENV=test nyc ava",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "ava": {
    "files": [
      "src/**/*.test.js"
    ],
    "babel": "inherit",
    "require": [
      "babel-register",
      "babel-polyfill"
    ]
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ]
  },
  "author": "Zach Orlovsky <sadorlovsky@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@sadorlovsky/eslint-config": "^0.13.0",
    "ava": "^0.17.0",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.20.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-webpack-loaders": "^0.8.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "coveralls": "^2.11.9",
    "enzyme": "^2.3.0",
    "eslint": "^3.12.0",
    "eslint-plugin-ava": "^4.0.0",
    "eslint-plugin-fp": "^2.2.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.8.0",
    "exports-loader": "^0.6.3",
    "file-loader": "^0.9.0",
    "if-env": "^1.0.0",
    "imports-loader": "^0.7.0",
    "json-loader": "^0.5.4",
    "nyc": "^10.0.0",
    "react-addons-test-utils": "^15.1.0",
    "react-hot-loader": "3.0.0-beta.6",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "dependencies": {
    "express": "^4.14.0",
    "glamor": "^2.20.12",
    "lodash": "^4.13.1",
    "moment": "^2.13.0",
    "random-item": "^1.0.0",
    "react": "^15.1.0",
    "react-document-title": "^2.0.2",
    "react-dom": "^15.1.0",
    "react-icons": "^2.1.0",
    "react-router": "^3.0.0",
    "react-router-scroll": "^0.4.1"
  }
}
