{
  "name": "orlovsky.rocks",
  "version": "0.3.3",
  "description": "Zach Orlovsky's homepage",
  "scripts": {
    "start:dev": "babel-node server/dev.js",
    "start:prod": "npm run build && node dist/server.bundle.js",
    "build:server": "webpack --config webpack/webpack.server.config.js",
    "build:client": "webpack -p --config webpack/webpack.prod.config.js",
    "build": "npm run build:client && npm run build:server",
    "lint": "eslint src server test",
    "test": "CONFIG=$(pwd)/webpack/webpack.test.config.js BABEL_DISABLE_CACHE=1 NODE_ENV=test nyc ava",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "ava": {
    "files": [
      "src/**/*.test.js"
    ],
    "babel": "inherit",
    "require": [
      "babel-register",
      "babel-polyfill"
    ]
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ]
  },
  "author": "Zach Orlovsky <sadorlovsky@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@sadorlovsky/eslint-config": "^0.8.1",
    "ava": "^0.15.1",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-webpack-loaders": "^0.5.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "coveralls": "^2.11.9",
    "css-loader": "^0.23.1",
    "enzyme": "^2.3.0",
    "eslint": "^2.11.1",
    "eslint-plugin-ava": "^2.4.0",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-react": "^5.1.1",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "if-env": "^1.0.0",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "lost": "^6.8.0",
    "nyc": "^6.4.4",
    "postcss-cssnext": "^2.5.2",
    "postcss-font-magician": "^1.4.0",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.9.1",
    "react-addons-test-utils": "^15.1.0",
    "react-hot-loader": "^3.0.0-beta.0",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "sugarss": "^0.1.4",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "dependencies": {
    "express": "^4.13.4",
    "lodash": "^4.13.1",
    "moment": "^2.13.0",
    "react": "^15.1.0",
    "react-css-modules": "^3.7.6",
    "react-document-title": "^2.0.1",
    "react-dom": "^15.1.0",
    "react-icons": "^2.1.0",
    "react-markdown": "^2.2.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "react-router-redux": "^4.0.4",
    "react-router-scroll": "^0.2.0",
    "redux": "^3.5.2",
    "redux-actions": "^0.9.1",
    "redux-thunk": "^2.1.0"
  }
}
